<div class="container top-pad big">
  <h1>1. Choose a template</h1>
  <div class="row">
    <% for (var i=0; i<templates.length; i++) { %>
      <% var template = templates[i]; %> 
      <div class="col-sm-3 spaced">
        <div class="box hover fixed-height js-template" data-cid="<%= template.get('cid') %>">
          <p class="bold"><%= template.get('name') %></p>
          <!-- <p class="small italic grey">
            Created on: <%= template.attributes.created %>
          </p> -->
          <img class="full-width" src="<%= template.get('thumbnail_url') %>">
        </div>
      </div>
    <% } %>
  </div>
  <hr>
  <h1 class="spaced">2. Upload your forms</h1>
  <p class="bold red big spaced">Make sure they match the template you chose!</p>
  <input id="files" type="file" name="files[]" data-url="/upload_files" class="spaced" multiple>
  <button id="submit">Submit</button>
  <div class="progress">
    <div id="upload-progress" class="progress-bar progress-bar-info" role="progressbar" style="width: 0%; min-width: 30px;">0%</div>
  </div>
  <p id="status"></p>
  <p id="readiness"></p>
  <p id="price"></p>
  <button id="submit-job" style="display: none;">Submit job</button>
</div>
<br><br><br><br><br><br><br><br><br><br><br>
